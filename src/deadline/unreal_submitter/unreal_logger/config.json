{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "%(asctime)s [%(levelname)-8.8s] %(message)s"
    }
  },
  "handlers": {
    "stdout": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "default",
      "stream": "ext://sys.stdout"
    },
    "unreal": {
      "class": "deadline.unreal_submitter.unreal_logger.handlers.UnrealLogHandler",
      "level": "DEBUG"
    }
  },
  "loggers": {
    "unreal_logger": {"level":  "DEBUG", "handlers":  ["stdout", "unreal"]}
  }
}